{
  "git": {
    "tagName": "v${version}",
    "changelog": "auto-changelog --stdout --commit-limit false --unreleased --template https://raw.gitmirror.com/release-it/release-it/main/templates/changelog-compact.hbs"
  },
  "github": {
    "release": false,
    "skipLibCheck": true
  },
  "hooks": {
    "after:bump": "auto-changelog -p"
  },
  "plugins": {},
  "npm": {
    "timeout": 600000
  }
}
